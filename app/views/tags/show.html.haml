.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")
.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li{class: "#{'active' if current_page?(popular_index_path)}"}
          = link_to "Popular PROTO", popular_index_path
        %li{class: "#{'active' if current_page?(root_path)}"}
          = link_to "Newest PROTO", root_path
      = button_to "View Tags", tags_path, class: "btn btn-default"
.container.proto-list
  .row
    - @prototypes.each do |proto|
      = render partial: 'layouts/prototype', locals: { proto: proto }
